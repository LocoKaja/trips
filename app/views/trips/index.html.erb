<h1>Listing trips</h1>

<table>
  <thead>
    <tr>
      <th>Picture</th>
      <th>Start</th>
      <th>End</th>
      <th>Location</th>
      <th>Ocation</th>
      <th>Travellor</th>
      <th>Details</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @trips.each do |trip| %>
      <tr>
        <td><%= trip.picture %></td>
        <td><%= trip.start %></td>
        <td><%= trip.end %></td>
        <td><%= trip.location %></td>
        <td><%= trip.ocation %></td>
        <td><%= trip.travellor %></td>
        <td><%= trip.details %></td>
        <td><%= link_to 'Show', trip %></td>
        <td><%= link_to 'Edit', edit_trip_path(trip) %></td>
        <td><%= link_to 'Destroy', trip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trip', new_trip_path %>
